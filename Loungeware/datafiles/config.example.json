// to enable testing, copy this file and rename it config.dev.json . Place it right next to this file. 
// if config.dev.json exists, the game will read it.
{
    //Default false | Whether or not to run the game in test mode
    "test_mode_on": false,
    
    // Default "" | This is the filename for your microgame's config file (minus the ".json").
    // Example: If my microgame config file was "sam_cookiedunk.json", the microgame key would be "sam_cookiedunk".
    "microgame_key": "",   
    
    // Default false | Whether or not to instantly restart the test microgame when it ends
    "loop_game": false,  
    
    // Default 1 | The difficulty level to run the test at (1-5). 
    "difficulty_level": 1,
    
    // Default false | if true, mutes all audio when running the game in test mode
    "mute_test": false     
}
